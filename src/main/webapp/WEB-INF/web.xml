<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaeehttp://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
	<display-name>rocp-project</display-name>
	<welcome-file-list>

	</welcome-file-list>

	<servlet>
		<display-name>UserServlet</display-name>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.revature.servlets.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/users</url-pattern>

	</servlet-mapping>

	<servlet>
		<display-name>AccountServlet</display-name>
		<servlet-name>AccountServlet</servlet-name>
		<servlet-class>com.revature.servlets.AccountServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AccountServlet</servlet-name>
		<url-pattern>/accounts</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.revature.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>com.revature.servlets.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>WithdrawServlet</servlet-name>
		<servlet-class>com.revature.servlets.WithdrawServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WithdrawServlet</servlet-name>
		<url-pattern>/accounts/withdraw</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DepositServlet</servlet-name>
		<servlet-class>com.revature.servlets.DepositServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DepositServlet</servlet-name>
		<url-pattern>/accounts/deposit</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>TransferServlet</servlet-name>
